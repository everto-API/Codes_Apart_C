
#include <stdio.h>

int main(void) {
    char nome[20], parque[20];
    printf("Insira o seu nome e sobrenome.\n> ");
    scanf("%[^\n]s", &nome);
    printf("\nDigite o nome do local que você irá.\n> ");
    scanf(" %[^\n]s", &parque);
    printf("\nBem-vindo %s! Aguardamos você na/o %s", nome, parque);
}
