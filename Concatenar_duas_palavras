
#include <stdio.h>
#include <string.h>

int main() {
    char nome[20];
    char parque[20];
    fgets(nome, sizeof(nome), stdin);
    nome[strcspn(nome, "\n")] = 0;
    fgets(parque, sizeof(parque), stdin);
    parque[strcspn(parque, "\n")] = 0;
    printf("Bem-vindo %s! Aguardamos vocÃª na/o %s!", nome, parque);
    
}
